{
  "buildCommand": "pnpm install && pnpm prisma generate && pnpm build:client",
  "outputDirectory": "dist/spa",
  "installCommand": "pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
